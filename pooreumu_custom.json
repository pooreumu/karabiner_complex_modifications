{
  "title": "pooreumu custom",
  "rules": [
    {
      "description": "CAPS LOCK + shortcut",
      "manipulators": [
        {
          "description": "CAPS LOCK + k to down",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "k",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + i to up",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "i",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + j to left",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "j",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + l to right",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "l",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + space to spotlight (control + space)",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "spacebar",
              "modifiers": ["left_control"]
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + o to delete_or_backspace",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "o",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace"
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + u to delete_forward",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "u",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + period(.) to equal_sign(=)",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "period",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "equal_sign"
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + open_bracket([) to (",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "9",
              "modifiers": ["left_shift"]
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + close_bracket(]) to )",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "0",
              "modifiers": ["left_shift"]
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + a to left_shift",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "a",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_shift"
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + s to left_option",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "s",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_option"
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + d to left_command",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "d",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_command"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "caps_lock pressed",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "caps_lock pressed",
                "value": 0
              }
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
