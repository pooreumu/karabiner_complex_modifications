{
  "title": "pooreumu custom",
  "rules": [
    {
      "description": "CAPS LOCK + shortcut",
      "manipulators": [
        {
          "description": "CAPS LOCK + k to down",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
          "from": {
            "key_code": "k",
            "modifiers": {
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ],
          "type": "basic"
        },
        {
          "description": "CAPS LOCK + i to up",
          "conditions": [
            {
              "name": "caps_lock pressed",
              "type": "variable_if",
              "value": 1
            }
          ],
